{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/frontend/public/index.html"
    },
    {
      "src": "^/assessment$",
      "dest": "/frontend/build/index.html"
    },
    {
      "src": "^/assessment/(.*)$",
      "dest": "/frontend/build/index.html"
    }
  ]
}

